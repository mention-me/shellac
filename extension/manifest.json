{
  "name": "MM Shellac",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Toggle xdebug",
  "background" : {
    "page" : "background.html",
    "persistent" : true
  },
  "browser_action": {
    "default_icon": "images/icon--48.png",
    "default_title": "MM Shellac",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://127.0.0.1/*"
  ],
  "icons": {
    "16": "images/icon--16.png",
    "48": "images/icon--48.png",
    "128": "images/icon--128.png"
  }
}
